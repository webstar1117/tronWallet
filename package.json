{
    "name": "tron-create-address",
    "version": "0.1.10",
    "description": "Tron address & private key generator",
    "main": "build/lib/account.js",
    "types": "build/lib/account.d.ts",
    "directories": {
        "lib": "lib",
        "test": "test"
    },
    "scripts": {
        "test": "npx mocha -r ts-node/register 'test/**/*.ts'",
        "build": "npx tsc",
        "lint": "npx eslint '*/**/*.{ts,tsx}'",
        "lint:fix": "npx eslint '*/**/*.{ts,tsx}' --fix",
        "format": "prettier --write \"**/*.ts\"",
        "start": "node server.js"
    },
    "repository": {
        "type": "git",
        "url": "git+ssh://git@github.com/kushkamisha/tron-create-address.git"
    },
    "keywords": [
        "tron",
        "address",
        "account",
        "private key",
        "generator"
    ],
    "author": "Misha Kushka",
    "license": "MIT",
    "homepage": "https://github.com/kushkamisha/tron-create-address#readme",
    "devDependencies": {
        "@types/bs58": "^4.0.1",
        "@types/chai": "^4.2.14",
        "@types/elliptic": "^6.4.12",
        "@types/mocha": "^8.2.0",
        "@types/node": "^14.14.22",
        "@typescript-eslint/eslint-plugin": "^4.14.1",
        "@typescript-eslint/parser": "^4.14.1",
        "chai": "^4.2.0",
        "eslint": "^7.18.0",
        "eslint-config-prettier": "^7.2.0",
        "eslint-plugin-import": "^2.22.1",
        "eslint-plugin-prettier": "^3.3.1",
        "mocha": "^8.2.1",
        "prettier": "^2.2.1",
        "ts-node": "^9.1.1",
        "typescript": "^4.1.3"
    },
    "dependencies": {
        "axios": "^0.27.2",
        "cors": "^2.8.5",
        "ethers": "^5.6.9",
        "express": "^4.17.1",
        "is-mnemonics": "^1.0.0",
        "js-sha3": "^0.8.0",
        "nodemon": "^2.0.15",
        "tronweb": "^4.0.1"
    },
    "bugs": {
        "url": "https://github.com/kushkamisha/tron-create-address/issues"
    }
}
